drop table modelo,marca;
create table marca(
id serial primary key,
	nome varchar(100) not null unique
);
create table modelo(
id serial primary key,
	nome varchar(100) not null unique,
	marca_id int references marca(id) default 1
		on delete restrict
		on update cascade
);

insert into marca values (default,'Padr√£o')
insert into marca values(default,'VW');
insert into marca values(default,'FIAT');


insert into modelo values(default,'Uno',2);
select * from modelo;
